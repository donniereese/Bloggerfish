/* Dock Preferences                               */
/*------------------------------------------------*/
#dock {
	height: 72px;
	width: 512px;
	text-align: left;
/*	border: 2px solid #c4c4cf;*/
	margin: 0;
	padding: 0;
	}

#dockobjects {
	text-align: left;
/*	border: 2px solid #fff;*/
	list-style-type: none;
	margin: 0;
	padding: 0;
	}

.dockobject {
	position: relative;
	list-style: none;
	float: left;
	border: 0px solid #fff;
	width: 64px;
	height: 72px;
	vertical-align: bottom;
	background: url(images/desktop/dock/docktile64.png) no-repeat;
	}

.dockobject .do-title {
	position: absolute;
	left: 0px;
	bottom: 0px;
	width: 64px;
	height: 12px;
	text-align: center;
	color: #fff;
	font-weight:bolder;
	}

.dockobject .do-icon {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 64px;
	height: 64px;
	text-align: center;
	background-position: center;
	background-repeat: no-repeat;
	}

.dockobject .dbackground {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 128px;
	height: 128px;
	background: url(images/desktop/dock/docktile64.png);
	}

.dockobject .dicon {
	position: absolute;
	left: 0px;
	top: 0px;
	width: 128px;
	height: 128px;
	background: url(images/mechpencil.png);
	}
/*------------------------------------------------*/


